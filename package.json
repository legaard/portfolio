{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "Personal portfolio for Lasse Legaard",
  "main": "webpack.config.js",
  "scripts": {
    "start": "webpack-dev-server",
    "dev": "npm start",
    "prod": "npm run clean && npm run copy-assets && webpack -p --progress",
    "clean": "rm -rf build/ && mkdir build",
    "copy-assets": "cp -R src/assets build/assets"
  },
  "author": "Lasse Legaard",
  "license": "ISC",
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    }
  },
  "browserslist": [
    "> 1%",
    "Chrome > 20",
    "Firefox > 12",
    "Explorer >= 10",
    "Edge >= 12",
    "Safari > 6",
    "Opera >= 15",
    "iOS >= 6"
  ],
  "dependencies": {
    "axios": "^0.15.2",
    "bootstrap": "4.0.0-alpha.6",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-images": "^0.5.2",
    "react-loading": "^0.0.9",
    "react-router": "^2.8.1"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^3.10.1",
    "postcss-loader": "^1.3.2",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  }
}
